 {
     "builds": [
         {
             "src": "flavorsapi.py",
             "use": "@vercel/python",
              "config": {
                "memory": 256,
                "maxDuration": 60
            }
         }
     ],
     "routes": [
         {
             "src": "/(.*)",
             "dest": "flavorsapi.py"
         }
     ]
 }
